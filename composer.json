{
    "name": "asseco-voice/laravel-jwt-authentication",
    "description": "Package to decode and verify JWT tokens",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "illuminate/console": "^7.21",
        "illuminate/support": "^7.19",
        "lcobucci/jwt": "^3.3",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Voice\\Auth\\AuthUserServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Voice\\Auth\\": "src/"
        }
    }
}
