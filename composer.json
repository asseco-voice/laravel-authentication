{
  "name": "asseco-voice/authentication",
  "description": "Package to decode and verify JWT tokens",
  "license": "MIT",
  "require": {
    "illuminate/support": "^7.19",
    "lcobucci/jwt": "^3.3",
    "ext-json": "*",
    "illuminate/console": "^7.21",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "^1.3.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Voice\\Auth\\AuthUserServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Voice\\Auth\\": "src/"
    }
  }
}
